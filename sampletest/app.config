<?xml version="1.0" encoding="utf-8" ?>  
<configuration>  
    <startup>   
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />  
    </startup>  
    <appSettings>  
    <!-- Eligible values for Browser Names :   CHROME,IE-->
    <add key="BrowserName" value="IE"/>  
    <add key="BaseUrl" value="https://specflow.org/"/> 
    <!--In case we want to change paths for Drivers, change below path-->
    <add key="DriverPath" value="./Utilities/Drivers"/> 
    <add key="Environment" value="QA"/>
    <add key="AppUnderTest" value="Sample App 1"/>
    <add key="BaseURI" value="http://localhost:8080"/>
    <!--In case we want to change test reports folder, change below path-->
    <add key="ReportsPath" value="./Reports" />
  </appSettings>  
</configuration>